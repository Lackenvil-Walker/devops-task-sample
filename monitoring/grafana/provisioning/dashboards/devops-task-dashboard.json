{
  "uid": "devops-task",
  "title": "DevOps Task - App Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests (rate)",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 10
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (path)",
          "legendFormat": "{{path}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency (p95)",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 24,
        "h": 10
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}}"
        }
      ]
    }
  ],
  "annotations": {
    "list": []
  }
}